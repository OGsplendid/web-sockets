{
    "extends": "airbnb-base",
    "env": {
       "es6": true,
       "browser": true,
       "jest": true
    },
    "rules": {
      "no-restricted-syntax": [
        "error",
        "LabeledStatement",
        "WithStatement"
      ],
      "no-underscore-dangle": "off",
      "no-unused-vars": "off",
      "guard-for-in": "off",
      "no-confusing-arrow": "off"
    }
  }